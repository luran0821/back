.manage-amb,.manage-inquiryamb
  .center
    .top
      overflow hidden
      margin 0 auto
      width 200px
      margin-top 20px
      .btn
        float left
        width 100px
        padding 0
        height 35px
        line-height 35px
        a
          width 100%
          height 100%
          display block
  .content
    padding 20px 0 0 30px
    .searchuid
      input
        width 200px
    .textarea
      clear both
      float left
      margin-top 20px
      p
        float left
      textarea
        clear both
        float left
        width 300px
    .btn-pull
      clear both
      float left
      width 100px
      margin-top 30px
      margin-left 100px
.manage-inquiryamb
  .content
    padding 20px 0 0 30px
    .searchuid
      select
        width 80px
        float left
        height 35px
      input
        float left
        width 200px
        height 35px
        margin-right 15px
      .btn
        float left
  .center
    overflow hidden
    .col-lg-12
      padding 0
      margin-top 20px
  .maker
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
    .out
      width: 300px;
      height: auto;
      position: absolute;
      min-height: 150px;
      background: #fff;
      left: 50%;
      top: 50%;
      margin-left: -150px;
      margin-top: -100px;
      border-radius: 10px;
      padding: 25px;
      padding-top: 50px;
      p
        text-align center
      .btn
        width: 100px;
        margin: 0 auto;
        display: block;
        margin-top: 35px;