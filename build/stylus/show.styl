.view {
  position: relative;
  min-height: 100%;
  background: #efeff4; }

.pull-left {
  float: left; }

.pull-right {
  float: right; }

.clearfix:after {
  display: block;
  content: '';
  clear: both; }

.btn-holder {
  width: 100%;
  margin-top: 5px; }

.view {
  background-color: #fff; }

.page {
  padding: 5px 0; }

.list .card {
  display: block;
  position: relative;
  width: 100%;
  height: 195px;
  margin: 5px 0;
  background-color: #f0f0f0;
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center center; }
.list .card .corner-left {
  position: absolute;
  top: 0;
  left: 0;
  width: 45px;
  height: 45px; }
.list .card .card-footer {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 45px;
  padding-right: 50px;
  background-color: rgba(0, 0, 0, 0.5); }
.list .card .card-footer .info {
  padding-left: 15px; }
.list .card .card-footer .info > p {
  padding: 0;
  margin: 0;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap; }
.list .card .card-footer .info .title {
  margin-top: 6px;
  font-size: 14px;
  color: #fff; }
.list .card .card-footer .info .date {
  font-size: 12px;
  color: #a19c99; }
.list .card .card-footer .angle-right {
  position: absolute;
  bottom: 0;
  right: 0;
  width: 45px; }