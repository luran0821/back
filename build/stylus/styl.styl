*
  box-sizing border-box
  margin 0
html,body
  width 100%
  min-height 100%
//background #636A7D
ul,li
  list-style none
.none
  display none
.block
  display block
.fl,.left
  float left
.rl.right
  float right
.logo
  width 80px
#page-wrapper
  background #f8f8f8
  padding-left 0
  overflow hidden
.bs-docs-example
  position relative
  margin 15px 0
  padding 5px 19px 25px
  background-color #fff
  border: 1px solid #ddd
  -webkit-border-radius 4px
  -moz-border-radius 4px
  border-radius 4px
.title
  h4
    border 0
    margin 0
    padding 10px 0 0 20px
    font-size 15px
  ul
    margin-top 10px
    overflow hidden
    padding-left 20px
    border-bottom  1px solid #555
    li
      float left
      margin-right 15px
      margin-bottom -1px
      padding 0 5px 5px 5px
      a
        color #000000
    .active
      border-bottom 3px solid #000
.defconf
  .center
    padding 10px 20px
.index,.addressfance
  .center
    padding 10px 20px
    .top
      background #ddd
      padding-left 20px
      overflow hidden
      >div
        margin 20px
      ul
        overflow hidden
        padding 0
        li
          float left
          overflow hidden
          padding-top 10px
          margin-right 30px
          width 20%
          input
            width 100%
      h4
        font-size 14px
        margin-top 0
      .form-control
        width 100%
        height 35px
      .fl
        width 20%
        margin-right 30px
      .textarea
        textarea
          height auto
      .addimg
        overflow hidden
        width 260px
        .from1
          overflow hidden
          float left
          margin 0 10px
          a
            clear both
            float left
            width 60px
            position relative
            display inline-block
            overflow hidden
            height 20px
            padding 0
            margin 0
            margin-top 5px
            span
              float left
              width 100%
              line-height 20px
              display block
              font-size 12px
            input
              position absolute
              right 0
              top 0
              width 100%
              height 100%
              opacity 0
        input[type="file"]
          width 60px
          float left
          clear both
        .i-plus
          position relative
          float left
          width 45px
          height 45px
          border 1px solid #5a5a5a
          border-radius 2px
          margin 0 7.5px
          line-height 1.1
          text-align center
          font-size 34px
          padding 0
          cursor pointer
          i
            color #5a5a5a
            margin-right 1px
        .bgimg
          position absolute
          top 0
          left 0
          width 100%
          height 100%
          background-repeat no-repeat
          background-origin center center
          background-size cover
          background-position center
        .bgwhite
          background-color white
        ul
          float left
          li
            width 75px
            margin-right 0
          input
            width 60px
      .btn
        margin 0 auto
        width 120px
        display block
        margin-bottom 20px
    .map,.fancemap
      height 500px
      margin 20px 0 20px 0
    .btn-bottom
      width 180px
      display block
      margin 0  auto
      margin-bottom 30px
  .maker
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
    .out
      width: 300px;
      height: auto;
      position: absolute;
      min-height: 200px;
      background: #fff;
      left: 50%;
      top: 50%;
      margin-left: -150px;
      margin-top: -100px;
      border-radius: 10px;
      padding: 25px;
      padding-top: 50px;
      p
        text-align center
      .btn
        width: 100px;
        margin: 0 auto;
        display: block;
        margin-top: 35px;
  #myPageTop
    position: absolute;
    top: 5px;
    right: 10px;
    border-image-source: initial;
    border-image-slice: initial;
    border-image-width: initial;
    border-image-outset: initial;
    border-image-repeat: initial;
    font-family: "Microsoft Yahei", Pinghei;
    font-size: 14px;
    background: none 0px 0px repeat scroll rgb(255, 255, 255);
    border-width: 1px;
    border-style: solid;
    border-color: rgb(204, 204, 204);
    margin: 10px auto;
    padding: 6px;
    z-index: 1000;
