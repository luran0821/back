  .maker
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
    overflow scroll
    .out
      width: 300px;
      height: auto;
      position: absolute;
      min-height: 200px;
      background: #fff;
      left: 50%;
      top: 50%;
      margin-left: -150px;
      margin-top: -100px;
      border-radius: 10px;
      padding: 25px;
      padding-top: 50px;
    .btn
      margin: 30px 50px;
    >div
      padding 40px 30px
      background #FFFFFF
      height 200%
      width 50%
      margin 0 auto
      li
        width 100%
        float left