.user-manage
  .center
    .top
      padding 20px 40px
      overflow hidden
      .select
        .btn-primary
          width 100px
          margin 0 auto
          display block
      .input-sm
        width 260px
        margin-bottom 20px
    .col-lg-12
      //min-width 1150px
      width 100%
      margin-top 30px
      #tbody
        td
          line-height 1.5
  .maker
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
    .out
      width: 300px;
      height: auto;
      position: absolute;
      min-height: 200px;
      background: #fff;
      left: 50%;
      top: 50%;
      margin-left: -150px;
      margin-top: -100px;
      border-radius: 10px;
      padding: 25px;
      padding-top: 50px;
      .btn
        width: 100px;
        margin: 0 auto;
        display: block;
        margin-top: 35px;
