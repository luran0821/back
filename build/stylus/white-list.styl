.white-list,.white-query
  .center
    .top
      overflow hidden
      margin 0 auto
      width 200px
      margin-top 20px
      .btn
        float left
        width 100px
        padding 0
        height 35px
        line-height 35px
        a
          width 100%
          height 100%
          display block
    .content
      padding 20px 0 0 30px
      .searchuid
        input
          width 200px
      .checkbox
        overflow hidden
        margin 15px 0
        padding-left 0
        label
          float left
          clear both
        div
          clear both
          float left
          cursor pointer
          p
            width 16px
            height 16px
            border 1px solid #ccc
            border-radius 3px
            float left
            margin-right 10px
          span
            float left
      .textarea
        clear both
        float left
        p
          float left
        textarea
          clear both
          float left
          width 300px
      .btn-pull
        clear both
        float left
        width 100px
        margin-top 30px
        margin-left 100px
  .maker
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
    .out
      width: 300px;
      height: auto;
      position: absolute;
      min-height: 150px;
      background: #fff;
      left: 50%;
      top: 50%;
      margin-left: -150px;
      margin-top: -100px;
      border-radius: 10px;
      padding: 25px;
      padding-top: 50px;
      p
        text-align center
      .btn
        float left
        width: 100px;
        margin: 0 auto;
        display: block;
        margin-top: 35px;
      .btn-right
        float right
.white-query
  .searchuid
    overflow hidden
    margin 20px 0
    width auto
    padding-left 15px
    input
      float left
      width 200px
      height 35px
      margin-right 15px
    .btn
      float left
  .maker
    .out
      .btn
        float inherit
        margin 20px auto 0 auto
